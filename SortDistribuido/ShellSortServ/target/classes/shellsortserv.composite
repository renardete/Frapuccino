<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
  targetNamespace="http://frascati.ow2.org/helloworld-rmi"
  name="ShellSortServer">

  <component name="server">
 
    <service name="s">
      <interface.java interface="co.edu.icesi.shellsort.interfaces.DistSort"/>
      <frascati:binding.rmi host="localhost" serviceName="rmiservice" port="1099"/>
    </service>
 
    <implementation.java class="co.edu.icesi.shellsort.implementation.ShellSort" />
 
  </component>

</composite>


